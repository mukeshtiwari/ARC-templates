@InProceedings{10.1007/978-3-642-36594-2_18,
  author = {Bitansky, Nir and Chiesa, Alessandro and Ishai, Yuval and Paneth, Omer and Ostrovsky, Rafail},
  editor = {Sahai, Amit},
  title = {{Succinct Non-interactive Arguments via Linear Interactive Proofs}},
  booktitle = {Theory of Cryptography},
  year = {2013},
