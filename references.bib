@InProceedings{10.1007/978-3-642-17373-8_19,
author={Groth, Jens},
editor={Abe, Masayuki},
title={{Short Pairing-Based Non-interactive Zero-Knowledge Arguments}},
booktitle={Advances in Cryptology - ASIACRYPT 2010},
year={2010},
publisher={Springer Berlin Heidelberg},
address={Berlin, Heidelberg},
pages={321--340},
abstract={We construct non-interactive zero-knowledge arguments for circuit satisfiability 
with perfect completeness, perfect zero-knowledge and computational soundness. The non-interactive 
zero-knowledge arguments have sub-linear size and very efficient public verification. 
The size of the non-interactive zero-knowledge arguments can even be reduced to a constant 
number of group elements if we allow the common reference string to be large. Our constructions 
rely on groups with pairings and security is based on two new cryptographic assumptions; 
we do not use the Fiat-Shamir heuristic or random oracles.},
isbn={978-3-642-17373-8}
}
@InProceedings{10.1007/978-3-642-38348-9_37,
  author = {Gennaro, Rosario and Gentry, Craig and Parno, Bryan and Raykova, Mariana},
